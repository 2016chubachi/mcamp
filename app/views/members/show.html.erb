<% provide(:title, @member.user_name) %>
<% if flash.notice %>
  <div class="tn-box tn-box-color">
    <p id="p-tn-box"><%= flash.notice %></p>
  </div>
  <br/>
<% end%>

<!-- <p class="alert"><%= flash.alert %> -->
<% if flash.alert %>
  <p class="alert"><%= flash.alert %></p>
<% end%>
<div class="row">
  <aside class="col-md-4">
    <section class="member_info">
     <h1>
       <%= gravatar_for @member %>
       <%= @member.user_name %>
     </h1>
    </section>
    </aside>
</div>
